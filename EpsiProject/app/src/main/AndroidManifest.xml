<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.epsi.epsiproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".EpsiProjectApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EpsiProject">
        <activity android:name=".Product.ProductListWithWSActivity" />
        <activity android:name=".Categorie.CategorieWithWSActivity" />
        <activity android:name=".ImagesActivity" />
        <activity android:name=".ImagesProductActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>